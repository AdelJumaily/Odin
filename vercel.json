{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "DATABASE_HOST": "@database_host",
    "DATABASE_PORT": "@database_port",
    "DATABASE_NAME": "@database_name",
    "DATABASE_USER": "@database_user",
    "DATABASE_PASSWORD": "@database_password",
    "DATABASE_SSL": "true",
    "DATABASE_CONNECTION_LIMIT": "20"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}
